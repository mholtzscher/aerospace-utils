# Missing aerospace binary shows warning.

# Override PATH to exclude the real aerospace binary while keeping
# aerospace-utils available via TESTSCRIPT_BIN.
mkdir bin
env PATH=$WORK/bin:$TESTSCRIPT_BIN

exec aerospace-utils workspace adjust --config-path config.toml --state-path state.toml --monitor-width 1920 --no-color --by=10
stdout 'aerospace not found'

-- config.toml --
start-at-login = true

[gaps]
[gaps.inner]
horizontal = 10
vertical = 10

[gaps.outer]
top = 10
bottom = 10
left = [
    { monitor.main = 100 },
]
right = [
    { monitor.main = 100 },
]

-- state.toml --
[monitors.main]
current = 50
default = 50

[monitors.'Built-in Retina Display']
current = 75
default = 60
