# Missing config file returns error with helpful message.

! exec aerospace-utils workspace use --config-path nonexistent.toml --state-path state.toml --monitor-width 1920 --no-color 50
stderr 'config file not found'

-- state.toml --
[monitors.main]
current = 50
default = 50

[monitors.'Built-in Retina Display']
current = 75
default = 60
