# Empty state file without percentage fails.

! exec aerospace-utils gaps use --config-path config.toml --state-path state.toml --monitor-width 1920 --no-color
stderr 'no percentage'

-- config.toml --
start-at-login = true

[gaps]
[gaps.inner]
horizontal = 10
vertical = 10

[gaps.outer]
top = 10
bottom = 10
left = [
    { monitor.main = 100 },
]
right = [
    { monitor.main = 100 },
]

-- state.toml --
