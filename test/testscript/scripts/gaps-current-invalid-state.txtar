# Invalid state file format shows error.

exec aerospace-utils workspace current --config-path config.toml --state-path state.toml --no-color
stdout 'Error loading state'

-- config.toml --
start-at-login = true

[gaps]
[gaps.inner]
horizontal = 10
vertical = 10

[gaps.outer]
top = 10
bottom = 10
left = [
    { monitor.main = 100 },
]
right = [
    { monitor.main = 100 },
]

-- state.toml --
not=toml
